{
    "name": "webstrates-projects-chrome-extension",
    "version": "1.1.0",
    "description": "A Vue.js Chrome extension for Composing Webstrates Projects",
    "scripts": {
        "buildTest": "node utils/build.js",
        "build": "node utils/build.js && zip -r ext.zip ./build/*",
        "start": "node utils/webserver.js",
        "watch": "NODE_ENV=development webpack --config webpack.config.js --mode=development --watch",
        "test:client": "node tests/bootstrap.js",
        "test:puppeteer": "NODE_ENV=testing mocha-webpack --webpack-config webpack.config.js tests/main.test.js"
    },
    "dependencies": {
        "babel-preset-es2015": "^6.24.1",
        "css-loader": "^2.1.1",
        "fs-extra": "^8.0.1",
        "html-webpack-plugin": "3.2.0",
        "moment": "^2.29.4",
        "style-loader": "^0.23.1",
        "write-file-webpack-plugin": "4.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@vue/test-utils": "^1.0.0-beta.29",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.0-rc.22",
        "chai": "^4.2.0",
        "d3": "^5.9.2",
        "eslint": "^5.16.0",
        "file-loader": "^4.0.0",
        "jsdom": "15.1.1",
        "jsdom-global": "3.0.2",
        "mini-css-extract-plugin": "^0.7.0",
        "mocha": "^6.1.4",
        "mocha-jsdom": "^2.0.0",
        "mocha-webpack": "^2.0.0-beta.0",
        "puppeteer": "^1.8.0",
        "puppeteer-core": "^1.17.0",
        "sinon": "^7.3.2",
        "sinon-chrome": "^3.0.1",
        "vue": "^2.6.10",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10",
        "webpack": "4.33.0",
        "webpack-chrome-extension-reloader": "^1.3.0",
        "webpack-cli": "^3.3.3",
        "webpack-dev-server": "^3.6.0",
        "webpack-node-externals": "^1.7.2",
        "ws": "^7.0.0"
    }
}
